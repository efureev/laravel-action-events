{
  "name": "efureev/laravel-action-events",
  "description": "Allows you to store various event actions: model changes, events, just strings",
  "license": "MIT",
  "authors": [
    {
      "name": "Eugene Fureev",
      "email": "fureev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.38",
    "illuminate/database": "^8.38",
    "illuminate/events": "^8.38",
    "efureev/laravel-support": "^1.3",
    "efureev/laravel-support-db": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.17",
    "fakerphp/faker": "^1.14",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Fureev\\ActionEvents\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fureev\\ActionEvents\\Tests\\": "tests/",
      "Fureev\\ActionEvents\\Tests\\Database\\Factories\\": "tests/database/factories/"
    }
  },
  "scripts": {
    "phpcs": "@php ./vendor/bin/phpcs",
    "cs-fix": "@php ./vendor/bin/phpcbf",
    "test": "@php ./vendor/bin/phpunit --no-coverage --testdox",
    "test-cover": "@php ./vendor/bin/phpunit --coverage-text"
  }
}
